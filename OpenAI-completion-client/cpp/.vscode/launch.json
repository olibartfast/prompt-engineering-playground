// https://code.visualstudio.com/docs/editor/variables-reference
{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Multimodal c++ client",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/openai-completion-client",
            "args": ["-p", "Describe the contents of this image", "${workspaceFolder}/512.jpg", "-r", "together", "-m", "meta-llama/Llama-3.2-11B-Vision-Instruct-Turbo"],
            "environment": [
                { "name": "config", "value": "Debug" },
                { "name": "OPENAI_API_KEY", "value": "" },
                { "name": "OPENAI_ENDPOINT", "value": "https://api.openai.com/v1/chat/completions" },
                { "name": "TOGETHER_API_KEY", "value": "" },
                { "name": "TOGETHER_ENDPOINT", "value": "https://api.together.xyz/v1/chat/completions"},
                { "name": "VLLM_API_KEY", "value": "token123" },
                { "name": "VLLM_ENDPOINT", "value": "https://localhost:8080/v1/chat/completions"}
            ],
            "cwd": "${workspaceFolder}"
        }
          
    ]
}